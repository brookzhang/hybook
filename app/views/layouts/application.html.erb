<!DOCTYPE html>
<html>
  <head>
  <title><%= title %></title>
		<%= csrf_meta_tag %>
		<%= render 'layouts/stylesheets' %>
    <%=javascript_include_tag :defaults %>
	</head>
	<body>
  <center>
	<!-- Book: 頁面head -->
	<TABLE  cellSpacing=0 cellPadding=0  border=0 >
	  <tr>
	    <td width=67 class="bodyleft" ></td><!--左侧空白-->
	    <!--中间内容-->
	    <td width=800 bgcolor=#cbbb8d>
	    	
	      
				<%= render 'layouts/header'%>
				
				<% flash.each do |key,value| %>
          <div class="flash<%=key%>"><%=value%></div>
        <%end%>
				
				<%= yield %>
				
				<%= render 'layouts/footer'%>
      
				
				
			</td>
      <td width=67 style="BACKGROUND: url(images/bgright.gif) repeat-y center top; "></td><!--右侧空白-->
	  </tr>
	</table>
	<div><%= debug(params) if Rails.env.development? %></div>
	
	</body>
  </center>
</html>
  
		
		
