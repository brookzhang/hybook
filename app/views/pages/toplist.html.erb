<TABLE cellSpacing=0 cellPadding=0 class="TableContentBlock">
  <TR>
    <TD class="TdLeftMenuFrame" vAlign=top>
        <TABLE cellSpacing=0 cellPadding=0 width="100%" >
          <TR>
            <TD class="TdDarkBlueContainer" vAlign=top>&nbsp;排序方式</td>
          </tr>
          <TR>
            <TD  vAlign=top class="TdWhiteContainer">
               by all_click
               by month_click
               by week_click
               by day_click
               
            <div class="DivBottomBorderline"></div>
            <br>
            </td>
          </tr>
        </table>
      </td>
      
      <TD width="1px"></TD>
      
      <TD class="TdRightContentFrame" vAlign=top>
        
        <TABLE cellSpacing=0 cellPadding=0 width="100%"  >
          <TR>
            <TD class="TdDarkBlueContainer"  vAlign=top>&nbsp;Top list  </td>
          </tr>
          <TR>
            <TD class="TdWhiteContainer" vAlign=top >
            <TABLE cellSpacing=2 cellPadding=0 width="100%"  >
            <tr>
                <th width="40">点击</th><th align="left">title</th>
            </tr>
            <% @books.each do |book|%>
            <tr class="<%= cycle('TdListEvenLine', 'TdListOddLine') -%>">
                <td align="right"><%=book.all_click.to_s%> </td><td>[<%= link_to book.title,book_path(book)%>]   </td>
            </tr>
            <%end%>
            </table>
            </td>
          </tr>
            
        </table>
      </td>
    </tr>
</table>
